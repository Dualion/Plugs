<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

	<!-- Base application theme. -->
	<style name="AppTheme" parent="android:Theme.Holo.Light.DarkActionBar">
		<!-- Customize your theme here. -->
	</style>

	<style name="DualionTheme" parent="android:Theme.Holo.Light.DarkActionBar">
		<item name="android:editTextBackground">@drawable/dualiontheme_edit_text_holo_light</item>		
		<item name="android:textColorHighlight">@color/dualion_light</item>
		<item name="android:textSelectHandleLeft">@drawable/dualiontheme_text_select_handle_left</item>
		<item name="android:textSelectHandleRight">@drawable/dualiontheme_text_select_handle_right</item>
		<item name="android:textSelectHandle">@drawable/dualiontheme_text_select_handle_middle</item>
		<item name="android:autoCompleteTextViewStyle">@style/AutoCompleteTextViewDualionTheme</item>
		<item name="android:listChoiceIndicatorMultiple">@drawable/dualiontheme_btn_check_holo_light</item>
		<item name="android:listChoiceIndicatorSingle">@drawable/dualiontheme_btn_radio_holo_light</item>
		<item name="android:buttonStyle">@style/ButtonDualionTheme</item>
		<item name="android:imageButtonStyle">@style/ImageButtonDualionTheme</item>
		<item name="android:dropDownSpinnerStyle">@style/SpinnerDualionTheme</item>
		<item name="android:progressBarStyleHorizontal">@style/ProgressBarDualionTheme</item>
		<item name="android:seekBarStyle">@style/SeekBarDualionTheme</item>
		<item name="android:ratingBarStyle">@style/RatingBarDualionTheme</item>
		<item name="android:ratingBarStyleIndicator">@style/RatingBarBigDualionTheme</item>
		<item name="android:ratingBarStyleSmall">@style/RatingBarSmallDualionTheme</item>
		<item name="android:buttonStyleToggle">@style/ToggleDualionTheme</item>
		<item name="android:listChoiceBackgroundIndicator">@drawable/dualiontheme_list_selector_holo_light</item>
		<item name="android:activatedBackgroundIndicator">@drawable/dualiontheme_activated_background_holo_light</item>
		<item name="android:fastScrollThumbDrawable">@drawable/dualiontheme_fastscroll_thumb_holo</item>
	</style>

</resources>
